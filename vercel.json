{
    "version": 2,
    "builds": [{
        "src": "index.js",
        "use": "@vercel/node"
    }],
    "routes": [{
            "src": "/api/upload",
            "dest": "index.js"
        },
        {
            "src": "/isi",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}